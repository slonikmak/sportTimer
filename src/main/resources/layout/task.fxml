<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane minWidth="-Infinity" prefHeight="31.0" styleClass="item_header" stylesheets="@item.css">
         <children>
            <Label layoutX="14.0" layoutY="14.0">
               <graphic>
                  <FontAwesomeIconView fx:id="playLable" glyphName="PLAY" />
               </graphic>
            </Label>
            <Label fx:id="title" layoutX="81.0" layoutY="14.0" styleClass="title_task" text="Indian dance" AnchorPane.leftAnchor="30.0" />
            <Button layoutX="526.0" layoutY="7.0" mnemonicParsing="false" onAction="#deleteTask" styleClass="close_btn" AnchorPane.rightAnchor="5.0">
               <graphic>
                  <FontAwesomeIconView glyphName="CLOSE" styleClass="close_icon" />
               </graphic>
            </Button>
         </children>
      </AnchorPane>
      <HBox>
         <children>
            <CheckBox mnemonicParsing="false" />
            <HBox>
               <children>
                  <Label text="Time" />
                  <Label fx:id="timeLable" text="10:00" />
               </children>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </HBox>
            <HBox>
               <children>
                  <Label text="Pause" />
                  <Label fx:id="pauseLable" text="0:5" />
               </children>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </HBox>
            <HBox>
               <children>
                  <Label text="Times x" />
                  <Label fx:id="timesLable" text="5" />
               </children>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </HBox>
         </children></HBox>
   </children>
</VBox>
